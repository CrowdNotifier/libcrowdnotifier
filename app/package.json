    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.32",
    "typescript": "^4.1.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
